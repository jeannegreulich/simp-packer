<% $simpsetup::dns::servers.each |number| { %>
server<%= $number %>.<%= $simpsetup::dns::domain %>.   IN A   <%= $simpsetup::dns::fwdaddr %>.<%= $number %>
<% } %>
<% $simpsetup::dns::clients.each |number| { %>
ws<%= $number %>.<%= $simpsetup::dns::domain %>.   IN A   <%= $simpsetup::dns::fwdaddr %>.<%= $number %>
<% } %>

